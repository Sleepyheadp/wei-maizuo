<!--pages/index/Banner/Banner.wxml-->
<swiper 
    indicator-dots
    autoplay
    interval
    duration
    circular
    indicator-active-color="#ff5f16"
    class="swiper"
  >
    <block wx:for="{{banners}}" wx:key="item.id">
      <swiper-item>
        <image src="{{item.images.small}}" class="slide-image"/>
      </swiper-item>
    </block>
  </swiper>
